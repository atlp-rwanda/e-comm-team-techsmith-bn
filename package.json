  "main": "server.js",
  "scripts": {
    "build": "rm -rf build && babel src --out-dir build",
    "migrate": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo",
    "make-seed": "npx sequelize-cli seed:generate --name demo-user",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo",
    "lint:fix": "eslint --fix \"**/*.js\"",
    "dev": "npm run lint:fix && nodemon --exec babel-node src/server.js",
    "start": "npm run build && node build/server.js",
    "test": " nyc --reporter=lcov --reporter=text mocha ./src/test/*.test.js --require @babel/register --timeout 1000000 --exit ",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
